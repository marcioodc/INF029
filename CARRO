// Estrutura para armazenar os dados
typedef struct aluno {
    int a_matricula;
    char a_nome[50];
    char a_sexo[2];
    int a_data_nascimento;
    int a_cpf;
} aluno;

typedef struct professor {
    int p_matricula;
    char p_nome[50];
    char p_sexo;
    int p_data_nascimento;
    int p_cpf;
} professor;

typedef struct disciplina {
    char d_nome[50];
    int d_codigo;
    int d_semestre;
    char d_professor;
} disciplina;

