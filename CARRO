em bd criaria uma tabela
em vetor de inteiros
array lista ligada
tabela hash

// Estrutura para armazenar os dados
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
    

typedef struct infcar{
    int chassi;
    int ano;
} infcar;

int main(){

    infcar carro[tam];

    for(int i=0; i<tam; i++){
        printf("Informe o chassi do %d° carro: ", i+ 1);
        scanf("%d", &carro[i].chassi);
        getchar();
        
        printf("Informe o ano do %d° carro: ", i+ 1);
        scanf("%d", &carro[i].ano);
        getchar();
        printf("\n");
    }
    printf("\n-----Lista de carros-----\n");
    for(int i=0; i<tam; i++){
        printf("\nCarro %d", i+ 1);
        printf("\nChassi: %d", carro[i].chassi);
        printf("\nAno: %d\n", carro[i].ano);
        
    }
    
}